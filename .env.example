# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1

# Stripe Payment Gateway
STRIPE_KEY=your_stripe_secret_key_here

# Stormpath (Legacy - service discontinued)
STORMPATH_CLIENT_APIKEY_ID=your_stormpath_client_apikey_id_here
STORMPATH_CLIENT_APIKEY_SECRET=your_stormpath_client_apikey_secret_here
STORMPATH_APPLICATION_HREF=your_stormpath_application_href_here

# Twilio SMS Service
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=+1XXXXXXXXXX

# SendGrid Email Service
SENDGRID_API_KEY=your_sendgrid_api_key_here

# Twitter API
TWITTER_CONSUMER_KEY=your_twitter_consumer_key_here
TWITTER_CONSUMER_SECRET=your_twitter_consumer_secret_here
TWITTER_ACCESS_TOKEN=your_twitter_access_token_here
TWITTER_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret_here

# Push Notifications
GCM_API_KEY=your_gcm_api_key_here
APN_KEY_PATH=/path/to/your/apn/key.pem
APN_PASSPHRASE=your_apn_passphrase_here
APN_CERT_PATH=/path/to/your/apn/cert.pem
